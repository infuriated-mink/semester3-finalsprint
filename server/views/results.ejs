<!DOCTYPE html>
<html>
  <head>
    <title>Search Results</title>
  </head>
  <body>
    <h1>Virtual Vault Game Search</h1>
    <button>
      <a href="/search">Search Again</a>
    </button>
    <% gameResults.forEach(function(result, index) { %>
    <p>
      <%= index + 1 %>: <b><%= result.gametitle %></b>
      <ul>
        <li>
          Description: <%= result.description %>
        </li>
        <li> 
          In-store location: <%= result.instorelocation %>
        </li>
        <li>
          Price: $<%= result.price %>
        </li>
        <li>
          Genre: <%= result.genrename %>
        </li>
        <li>
          Platform: <%= result.consolename %>
        </li>
        <li>
          QOH: <%= result.quantity %>
        </li>

      </ul>
     
    </p>
    <hr />
    <% }); %>
  </body>
</html>
